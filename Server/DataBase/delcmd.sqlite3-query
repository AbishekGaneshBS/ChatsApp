PRAGMA foreign_keys = OFF;
DELETE FROM Users;
DELETE FROM UserToUserMessages;
DELETE FROM Groups;
DELETE FROM GroupMembers;
DELETE FROM GroupMessages;
DELETE FROM sqlite_sequence WHERE name='Users';
